<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts 
Configuration 1.3//EN" "http://struts.apache.org/dtds/struts-config_1_3.dtd">
<struts-config>
	<form-beans>
		<form-bean name="baiThiActionForm" type="form.BaiThiActionForm"></form-bean>
		<form-bean name="lanThiActionForm" type="form.LanThiActionForm"></form-bean>
		<form-bean name="cauHoiActionForm" type="form.CauHoiActionForm"></form-bean>
		<form-bean name="userActionForm" type="form.UserActionForm"></form-bean>
		<form-bean name="fileActionForm" type="form.FileActionForm"></form-bean>
		<form-bean name="nhapCauHoiActionForm" type="form.NhapCauHoiActionForm"></form-bean>
		<form-bean name="ketQuaThiActionForm" type="form.KetQuaThiActionForm"></form-bean>
		<form-bean name="thongKeActionForm" type="form.ThongKeActionForm"></form-bean>
		<form-bean name="thongKeChiTietActionForm" type="form.ThongKeChiTietActionForm"></form-bean>
		<form-bean name="thongKeBaiThiThanhTichActionForm" type="form.ThongKeBaiThiThanhTichActionForm"></form-bean>
		<form-bean name="thongKeBaiThiActionForm" type="form.ThongKeBaiThiActionForm"></form-bean>
	</form-beans>
	
	<global-forwards>
		<forward name="Error" path="/ErrorPage.jsp"></forward>
		<forward name="Login" path="/ErrorLoginPage.jsp"></forward>
		<forward name="Home" path="/Home.do" redirect="true"></forward>
	</global-forwards>
	
	<action-mappings>
		<action path="/ChiTietBaiThi" type="action.ShowBaiThiAction"
			name="baiThiActionForm" scope="request">
			<forward name="ShowBaiThi" path="/Test-Detail.jsp"></forward>
		</action>
		<action path="/LamBaiThi" name="baiThiActionForm" type="action.LamBaiThiAction" scope="request">
			<forward name="CheckPassword" path="/CheckPassword.jsp"></forward>
		</action>
		<action path="/LanThi" name="lanThiActionForm" type="action.ShowLanThiAction" scope="request">
			<forward name="ShowLanThi" path="/Test-Make.jsp"></forward>
		</action>
		<action path="/LuuDapAn" name="cauHoiActionForm"
			type="action.LuuDapAnAction" scope="request">
		</action>
		<action path="/QuanLyBaiThi" name="baiThiActionForm"
			type="action.ShowQuanLyTestAction" scope="request">
			<forward name="long" path="/QuanLy-Test.jsp"></forward>
		</action>
		<action path="/QuanLyThanhVien" name="userActionForm" type="action.ShowQuanLyUserAction" scope="request">
			<forward name="ShowUser" path="/QuanLy-User.jsp"></forward>
		</action>
		<action path="/ThemBaiThi" name="baiThiActionForm"
			type="action.ThemBaiThiAction">
			<forward name="thembaithi" path="/QuanLy-Test-CreateEdit.jsp"></forward>
			<forward name="long" path="/listBaiThi.do"
				redirect="true">
			</forward>
		</action>
		<action path="/QuanLyCauHoi" name="cauHoiActionForm"
			type="action.ShowQuanLyCauHoiAction" scope="request">
			<forward name="ShowQuanLyCauHoi" path="/QuanLy-Question.jsp"></forward>
		</action>
			<action path="/SuaCauHoi" name="cauHoiActionForm" type="action.SuaCauHoiAction" scope="request">
			<forward name="SuaCauHoi" path="/QuanLyCauHoi.do" redirect="true"></forward>
		</action>
		<action path="/XoaCauHoi" name="cauHoiActionForm" type="action.XoaCauHoiAction" scope="request">
			<forward name="ShowQuanLyCauHoi" path="/QuanLyCauHoi.do" redirect="true"></forward>
		</action>
		<action path="/ThemCauHoi" name="cauHoiActionForm" type="action.ThemCauHoiAction" scope="request">
			<forward name="ThemCauHoi" path="/QuanLyCauHoi.do" redirect="true"></forward>
			<forward name="ShowQuanLyCauHoi" path="/QuanLyCauHoi.do" redirect="true"></forward>
		</action>
		<action path="/DangNhap" name="userActionForm" type="action.DangNhapAction" scope="request">
		</action>
		<action path="/NhapFile" name="fileActionForm" type="action.NhapBangFileAction" scope="request">
			<forward name="ShowNhapCauHoi" path="/NhapCauHoi.do" redirect="true"></forward>
		</action>
		<action path="/BaiThi" name="baiThiActionForm" type="action.ShowDanhSachBaiThiAction" scope="request">
		<forward name="BaiThi" path="/Test-List.jsp"></forward>
		</action>	
		<action path="/CaNhan" name="userActionForm" type="action.ShowCaNhanAction" scope="request">
			<forward name="profile" path="/Profile.jsp"></forward>
		</action>	
		<action path="/KetQua" name="ketQuaThiActionForm" type="action.ShowKetQuaLanThiAction" scope="request">
			<forward name="KetQua" path="/Test-Result.jsp"></forward>
		</action>
		<action path="/NhapCauHoi" name="nhapCauHoiActionForm" type="action.ShowNhapCauHoiAction" scope="request">
			<forward name="ShowNhapCauHoi" path="/ImportQuestion.jsp"></forward>
		</action>
		<action path="/TKBaiThi" name="thongKeBaiThiActionForm" type="action.ShowTKBaiThiAction" scope="request">
			<forward name="ThongKeBaiThiUser" path="/QuanLy-Test-Statistic.jsp"></forward>
		</action>	
		<action path="/TKChiTietBaiThi" name="thongKeChiTietActionForm" type="action.ShowTKChiTietBaiThiAction" scope="request">
			<forward name="ThongKeChiTietBaiThi" path="/QuanLy-Test-Statistic-Detail.jsp"></forward>
		</action>		
		<action path="/TKBaiThiThanhTich" name="thongKeBaiThiThanhTichActionForm" type="action.ShowChiTietThanhTichCaNhanAction" scope="request">
			<forward name="ThongKeChiTietBaiThiThanhTich" path="/Profile-achievement.jsp"></forward>
		</action>
		<action path="/ThongKe" name="thongKeActionForm" type="action.ShowTKTongQuanAction" scope="request">
			<forward name="ThongKeTongQuan" path="/Statistic.jsp"></forward>
		</action>
		<action path="/Home" name="thongKeActionForm" type="action.ShowTrangChuAction" scope="request">
			<forward name="TrangChu" path="/Home.jsp"></forward>
		</action>
		<action path="/SuaBaiThi" name="baiThiActionForm" type="action.SuaBaiThiAction" scope="request">
			<forward name="long" path="/QuanLyBaiThi.do"
				redirect="true">
			</forward>
		</action>
		<action path="/SuaThanhVien" name="userActionForm" type="action.SuaThanhVienAction" scope="request">
			<forward name="update" path="/QuanLyThanhVien.do" redirect="true"></forward>
			<forward name="resetPass" path="/QuanLyThanhVien.do" redirect="true"></forward>
			<forward name="stopUser" path="/QuanLyThanhVien.do" redirect="true"></forward>
		</action>
		<action path="/ThemBaiThi" name="baiThiActionForm" type="action.ThemBaiThiAction" scope="request">
			<forward name="thembaithi" path="/QuanLy-Test-CreateEdit.jsp"></forward>
			<forward name="long" path="/QuanLyBaiThi.do"
				redirect="true">
			</forward>
		</action>
		<action path="/XoaBaiThi" name="baiThiActionForm" type="action.XoaBaiThiAction" scope="request">
			<forward name="long" path="/QuanLyBaiThi.do"
				redirect="true">
			</forward>
		</action>
		<action path="/XoaLanThi" name="lanThiActionForm" type="action.XoaLanThiAction" scope="request">
		</action>
		<action path="/GetBaiThi" name="baiThiActionForm" type="action.GetBaiThiAction" scope="request">
		</action>
		<action path="/GetCauHoi" name="cauHoiActionForm" type="action.GetCauHoiAction" scope="request">
		</action>
		<action path="/XuLyCauHoi" name="nhapCauHoiActionForm" type="action.NhapCauHoiAction" scope="request">
		</action>
		<action path="/DangXuat" name="userActionForm" type="action.DangXuatAction" scope="request">
		</action>
	</action-mappings>
	<message-resources parameter="action.ApplicationResources" ></message-resources>
	
</struts-config>
