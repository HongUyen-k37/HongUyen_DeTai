<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts 
Configuration 1.3//EN" "http://struts.apache.org/dtds/struts-config_1_3.dtd">
<struts-config>
	<form-beans>
		<form-bean name="dangNhapActionForm" type="form.NguoiDungActionForm" />
		<form-bean name="dangXuatActionForm" type="form.NguoiDungActionForm" />
		<form-bean name="danhSachKyThiActionForm" type="form.KyThiActionForm" />
		<form-bean name="danhSachThiSinhActionForm" type="form.ThiSinhActionForm" />
	</form-beans>
	<action-mappings>
		<action path="/DangNhap" name="dangNhapActionForm"
				type="action.DangNhapAction" scope="request">
			<forward name="success" path="/KyThi.do" redirect="true"/>
			<forward name="error" path="/DangNhap.jsp" />
		</action>
		<action path="/DangXuat" name="dangXuatActionForm"
				type="action.DangXuatAction" scope="request">
			<forward name="success" path="/DangNhap.jsp" />
		</action>
		<action path="/KyThi" name="danhSachKyThiActionForm"
				type="action.DanhSachKyThiAction" scope="request">
			<forward name="success" path="/KyThi.jsp" />
			<forward name="error" path="/DangNhap.jsp" />
		</action>
		<action path="/ThiSinh" name="danhSachThiSinhActionForm"
				type="action.DanhSachThiSinhAction" scope="request">
			<forward name="success" path="/ThiSinh.jsp" />
			<forward name="error" path="/DangNhap.jsp" />
		</action>
		<action path="/SoBaoDanh" name="danhSachThiSinhActionForm"
				type="action.DanhSachThiSinhAction" scope="request">
			<forward name="success" path="/ThiSinh.jsp" />
			<forward name="error" path="/DangNhap.jsp" />
		</action>
	</action-mappings>
	<message-resources parameter="action.ApplicationResources" />
</struts-config>
